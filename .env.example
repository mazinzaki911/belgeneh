# Supabase Configuration (Client-side)
# Get these from your Supabase project settings: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Configuration (Server-side)
# IMPORTANT: Never expose SUPABASE_SERVICE_ROLE_KEY on the client-side!
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# PostgreSQL Database Configuration
# Get these from Supabase project settings: https://app.supabase.com/project/_/settings/database
POSTGRES_URL=your_postgres_connection_string_with_pooling
POSTGRES_PRISMA_URL=your_postgres_connection_string_for_prisma
POSTGRES_URL_NON_POOLING=your_postgres_connection_string_direct
POSTGRES_USER=postgres
POSTGRES_HOST=your_postgres_host
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=postgres

# Gemini AI API Key (Optional)
# Get this from Google AI Studio: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key
